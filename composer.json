{
    "name": "bike/api",
    "description": "bike api project",
    "license": "MIT",
    "require": {
        "php": ">=7.0.0",
        "slim/slim": "^3.1",
        "slim/php-view": "^2.0",
        "monolog/monolog": "^1.17",
        "doctrine/dbal": "^2.3",
        "league/oauth2-server": "^5.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.8 < 6.0"
    },
    "autoload": {
        "psr-4": {
            "Bike\\Api\\": "src/"
        },
        "files": [
            "lib/aliyun/mns-autoloader.php",
            "lib/alipay/AopSdk.php",
            "lib/jpush/jpush-api-php-client-3.5.12/autoload.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "process-timeout" : 0
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://packagist.phpcomposer.com"
        }
    }
}
